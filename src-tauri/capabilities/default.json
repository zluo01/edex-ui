{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "os:default",
    "store:default",
    "shell:default",
    "log:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://ip-api.com/json/*"
        },
        {
          "url": "https://1.1.1.1/*"
        }
      ]
    }
  ]
}
