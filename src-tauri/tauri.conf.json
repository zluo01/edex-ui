{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "edex",
  "mainBinaryName": "edex",
  "version": "0.0.1",
  "identifier": "edex",
  "build": {
    "beforeDevCommand": "pnpm run vite-dev",
    "beforeBuildCommand": "pnpm run vite-build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1874"
  },
  "app": {
    "windows": [
      {
        "fullscreen": true,
        "resizable": true,
        "title": "EDEX-UI",
        "maximized": true,
        "minWidth": 1280,
        "minHeight": 720
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/256x256.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
