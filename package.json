    "@types/lodash": "4.17.23",
    "@types/node": "24.10.13",
    "@typescript-eslint/parser": "8.54.0",
    "eslint": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-solid": "0.14.5",
    "globals": "17.3.0",
    "husky": "9.1.7",
