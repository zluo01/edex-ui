    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vite": "7.3.1",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-solid": "2.11.10",
    "vite-tsconfig-paths": "6.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --cache --fix",
      "pnpm run lint",
      "pnpm run format"
    ],
    "*.{js,css,md}": "prettier --write"
